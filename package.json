{
  "name": "occamsrazor-broadcast-browser",
  "version": "0.0.2",
  "description": "it adds broadcast capability when using occamsrazor as event bus in the browser",
  "main": "index.js",
  "scripts": {
    "test": "mocha tests/**/*.js",
    "watch": "npm run test -- -w",
    "lint": "./node_modules/.bin/eslint --ext .js ./src ./tests",
    "release:major": "./node_modules/.bin/npm-release major",
    "release:minor": "./node_modules/.bin/npm-release minor",
    "release:patch": "./node_modules/.bin/npm-release patch",
    "precommit": "npm run lint",
    "prepush": "npm run test"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/sithmel/occamsrazor-broadcast-browser.git"
  },
  "keywords": [
    "events",
    "localstorage"
  ],
  "author": "Maurizio Lupo <maurizio.lupo@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "chai": "^1.10.0",
    "eslint": "^1.10.3",
    "husky": "^0.10.2",
    "mocha": "^2.1.0",
    "node-localstorage": "^1.3.0",
    "npm-release": "^1.0.0",
    "occamsrazor": "^7.0.0"
  },
  "peerDependencies": {
    "occamsrazor": "^7.0.0"
  }
}
